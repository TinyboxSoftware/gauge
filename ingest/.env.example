# Railway API Configuration
# Get your API token from Railway dashboard: https://railway.app/account/tokens
RAILWAY_API_TOKEN=your_railway_api_token_here

# Your Railway Customer ID (UUID format)
RAILWAY_CUSTOMER_ID=your_customer_uuid_here

# Your Railway Workspace ID (UUID format)
RAILWAY_WORKSPACE_ID=your_workspace_uuid_here

# PostgreSQL Database Connection
# Format: postgresql://username:password@host:port/database
# On Railway, this is automatically provided as DATABASE_URL
DATABASE_URL=postgresql://user:password@localhost:5432/railway_metrics

# Log Level
# used for determining how your logs get output
LOG_LEVEL=debug
